[
    {
        "template": "virgo-std.tpl",
        "resources": true,
        "resourceNamePattern": "virgo-${server}-server-${virgo}.RELEASE.*",
        "name": "${os}_${java}_${server}_${virgo}",
        "repository": "rstiller/virgo",
        "variables": [
            [
                { "os": "debian-squeeze" },
                { "os": "debian-wheezy" },
                { "os": "ubuntu-12.04" },
                { "os": "ubuntu-12.10" },
                { "os": "ubuntu-13.04" },
                { "os": "ubuntu-13.10" },
                { "os": "ubuntu-14.04" }
            ],
            [
                { "java": "openjdk-7-jdk" },
                { "java": "oracle-7-jdk" },
                { "java": "oracle-8-jdk" }
            ],
            [
                { "server": "tomcat" },
                { "server": "jetty" }
            ],
            [
                { "virgo": "3.0.3" },
                { "virgo": "3.5.0" },
                { "virgo": "3.6.2" }
            ]
        ]
    },
    {
        "template": "virgo-nano.tpl",
        "resources": true,
        "resourceNamePattern": "virgo-${server}-full-${virgo}.RELEASE.*",
        "name": "${os}_${java}_${server}_${virgo}",
        "repository": "rstiller/virgo",
        "variables": [
            [
                { "os": "debian-squeeze" },
                { "os": "debian-wheezy" },
                { "os": "ubuntu-12.04" },
                { "os": "ubuntu-12.10" },
                { "os": "ubuntu-13.04" },
                { "os": "ubuntu-13.10" },
                { "os": "ubuntu-14.04" }
            ],
            [
                { "java": "openjdk-7-jdk" },
                { "java": "oracle-7-jdk" },
                { "java": "oracle-8-jdk" }
            ],
            [
                { "server": "nano" }
            ],
            [
                { "virgo": "3.5.0" },
                { "virgo": "3.6.2" }
            ]
        ]
    }
]
